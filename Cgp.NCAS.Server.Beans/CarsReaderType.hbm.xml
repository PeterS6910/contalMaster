<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.NCAS.Server.Beans.CarsReaderType, Cgp.NCAS.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="IdCarsReaderType">
      <generator class="guid" />
    </id>
    <property name="CameraTypeCode" not-null="true" unique="true" />
    <property name="Model" not-null="false" />
    <property name="Description" not-null="false">
      <column name="Description" sql-type="NVARCHAR(MAX)" />
    </property>
    <property name="Version" not-null="false" />
    <join table="CentralNameRegister">
      <key column="Id" foreign-key="none" />
      <property name="Name" not-null="true" />
      <property name="ObjectTypeCode" not-null="true" />
    </join>
  </class>
</hibernate-mapping>
